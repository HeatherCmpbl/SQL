--display the database ordered by price
Select * FROM popculture ORDER BY price;

--display the database of items priced over 14.99
SELECT * FROM popculture WHERE price > 14.99;

display the database of items priced over 14.99 in order by price
SELECT * FROM popculture WHERE price > 14.99 ORDER BY Price;
